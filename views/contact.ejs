    <div class="form-group col">
      <input class="form-control" type="email" placeholder="Email">
    </div>
  </div>
  <div>
    <div class="form-group">
      <textarea class="form-control" name="editor1" rows="7" placeholder="Message"></textarea>
    </div>
  </div>
</form>
</div>


